             xmlns:controls="clr-namespace:UiFramework.V2.Forms.Controls"
             x:Class="UiFramework.V2.Forms.Pages.SnippetPage"
             ios:Page.UseSafeArea="True">

    <controls:SnippetView ItemTappedCommand="{Binding ItemTappedCommand}"
                          SnippetLayout="{Binding SnippetLayout}" />
</ContentPage>